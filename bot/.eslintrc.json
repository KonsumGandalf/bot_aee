{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@javascript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@javascript-eslint"
    ],
    "rules": {
        "quotes" : "off",
        "linebreak-style": "off",
        "object-curly-spacing": "off",
        "max-len": "off"
    }
}
